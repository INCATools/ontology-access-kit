        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:integer ;
            owl:onProperty ontoenrich:background_count ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:integer ;
            owl:onProperty ontoenrich:sample_total ] ;
    skos:definition "A single enrichment result" .



